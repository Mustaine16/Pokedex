{
  "extends": ["airbnb-base", "plugin:flowtype/recommended", "prettier"],
  "rules": {
    "flowtype/define-flow-type": 2,
    "arrow-parens": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "no-confusing-arrow": 0
  },
  "parser": "babel-eslint",
  "plugins": ["flowtype"],
  "globals": {
    "window": true,
    "document": true
  }
}
